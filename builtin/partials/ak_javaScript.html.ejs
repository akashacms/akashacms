<% // drag in site-defined javascript
if (javaScripts)
for (var i in javaScripts) {
    var script = javaScripts[i];
    %><script <%
        if (script.lang) { %>type="<%= script.lang %>" <% }
        if (script.href) { %>src="<%= script.href %>" <% }
        %> > <%
        if (script.script) { %>
<%= script.script %>
<% } %></script>
<% } %>