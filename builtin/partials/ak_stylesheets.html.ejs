<%
// drag in site-defined stylesheets
if (stylesheets) {
var keys = Object.keys(stylesheets);
for (var i = 0; i < keys.length; i++) {
    var style = stylesheets[keys[i]];
    %><link rel="stylesheet" href="<%= style.href %>" type="text/css" <%
        if (style.media) { %> media="<%= style.media %>" <% } %> ></link>
<% }
} %>